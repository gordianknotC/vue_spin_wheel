{
  "name": "portfolio-branding",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "simu": "vue-cli-service serve --mode simulation",
    "lint": "CYPRESS_INSTALL_BINARY=0 vue-cli-service lint",
    "cypress:hotreload": "cypress open",
    "cypress:reload": "cypress open --config watchForFileChanges=false",
    "develop": "CYPRESS_INSTALL_BINARY=0 vue-cli-service build --mode develop",
    "prettier": "CYPRESS_INSTALL_BINARY=0 prettier --write 'src/**/*.vue' 'src/**/*.ts'",
    "production": "CYPRESS_INSTALL_BINARY=0 vue-cli-service build --mode production",
    "release": "CYPRESS_INSTALL_BINARY=0 vue-cli-service build --mode release",
    "gh_pages": "CYPRESS_INSTALL_BINARY=0 vue-cli-service build --mode gh_pages",
    "tailwind": "npx tailwindcss build src/assets/css/tailwind.css -o .tailwind/tailwind.css",
    "app:develop"   : "VUE_APP_ENV=development VUE_APP_MOBILE=true ANDROID_SYNC=true vue-cli-service build",
    "app:release"   : "VUE_APP_ENV=release VUE_APP_MOBILE=true ANDROID_SYNC=true vue-cli-service build",
    "app:production": "VUE_APP_ENV=production VUE_APP_MOBILE=true ANDROID_SYNC=true vue-cli-service build",
    "                         -" : "",
    " 安 裝 編 譯 套 件 及 設 定 " : "",
    "install:buildEssentials": "sudo yarn global add npx && yarn global add cordova-res && yarn global add npm-run-all && brew update && brew upgrade && brew cleanup -d -v && brew install cocoapods",
    "cap:init": "sudo npx cap init && run-s cap:addAndroid cap:addIOS",
    "cap:addAndroid": "sudo rm -r -f android && npx cap add android",
    "cap:addIOS": "sudo rm -r -f ios && npx cap add ios",
    "           -_": "",
    "-------------": "",
    "cap:android": "npx cap run android -l --external",
    "cap:ios": "npx cap run ios -l --external",
    "cap:copy": "npx cap copy",
    "cap:sync": "npx cap copy && npx cap sync",
    "open:android": "npx cap open android",
    "open:ios": "npx cap open ios",
    "            _": "",
    "------------_": "",
    "run:android:dev": "yarn app:develop && yarn cap:sync develop && yarn cap:android",
    "run:android:prod": "yarn app:production && yarn cap:sync production && yarn cap:android",
    "run:android:rels": "yarn app:release && yarn cap:sync release && yarn cap:android",
    "run:ios:dev": "yarn develop && yarn cap:sync develop && yarn cap:ios",
    "run:ios:prod": "yarn production && yarn cap:sync production && yarn cap:ios",
    "run:ios:rels": "yarn release && yarn cap:sync release && yarn cap:ios",
    "update:icons": "cordova-res ios --skip-config --copy && cordova-res android --skip-config --copy"
  },
  "dependencies": {
    "@vant/touch-emulator": "1.3.2",
